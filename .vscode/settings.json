    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable",
                "settings": { "foreground": "#93CEEE" }  // 变量的颜色
            },
            {
                "scope": "variable.parameter",
                "settings": { "foreground": "#93CEEE" }  // 函数参数
            },
            {
                "scope": "entity.name.function",
                "settings": { "foreground": "#F9AE57" }  // 方法（函数）名称
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#41bfac",
                    // "fontStyle": "bold",
                }  // 类名、接口名
            },
            {
                "scope": "entity.name.namespace",
                "settings": { "foreground": "#41bfac" }  // 命名空间
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#c695c6",
                    // "fontStyle": "bold"
                }  // 关键字（如 `int`, `void`, `class`）
            },
            {
                "scope": "storage.type.namespace.definition",
                "settings": {
                    "foreground": "#ec6066",
                    // "fontStyle": "bold"
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#ec6066",
                    // "fontStyle": "bold"
                }  // 关键字（如 `if`, `else`, `return`）
            },
            {
                "scope": "keyword.other.using",
                "settings": {
                    "foreground": "#ec6066"
                }
            },
            {
                "scope": "keyword.other.",
                "settings": {
                    "foreground": "#ec6066",
                }
            },
            {
                "scope": "string.quoted.other.lt-gt.include.cpp",
                "settings": {
                    "foreground": "#F9AE57",
                }
            },
        ],
    },
    "editor.fontFamily": "Fira Code Medium, Microsoft Yahei",
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "Consolas",
}